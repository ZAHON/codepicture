{
  "name": "bluloco-light",
  "type": "light",
  "colors": {
    "--sandbox-editor-background": "#f9f9f9",
    "--sandbox-editor-foreground": "#383a42",
    "--sandbox-editor-cursor-foreground": "#f31459",
    "--sandbox-editor-selection-background": "#d2ecff80",
    "--sandbox-editor-line-number-inactive-foreground": "#a0a1a7",
    "--sandbox-window-header-background": "#e8e8e8",
    "--sandbox-window-tab-active-foreground": "#383a42",
    "--sandbox-window-tab-inactive-foreground": "#a0a1a7"
  },
  "tokenColors": [
    {
      "scope": "comment.unused.elixir",
      "settings": {
        "foreground": "#383A42"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation punctuation.definition",
        "string.comment",
        "comment.block.documentation",
        "comment.block"
      ],
      "settings": {
        "foreground": "#A0A1A7"
      }
    },
    {
      "scope": [
        "comment.block.documentation variable",
        "keyword.other.documentation",
        "storage.type.class.jsdoc",
        "comment.block variable.parameter",
        "keyword.other.phpdoc",
        "comment.block.documentation entity.name.type",
        "meta.other.type.phpdoc support class"
      ],
      "settings": {
        "foreground": "#727378",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "punctuation.comma.graphql",
        "punctuation.definition.variable",
        "punctuation.definition.parameters",
