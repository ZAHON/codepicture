      "settings": {
        "foreground": "#C5A332",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.numeric", "variable.other.anonymous.elixir"],
      "settings": {
        "foreground": "#CE33C0"
      }
    },
    {
      "scope": "none",
      "settings": {
        "foreground": "#CE33C0"
      }
    },
    {
      "scope": "none",
      "settings": {
        "foreground": "#0098DD"
      }
    },
    {
      "scope": [
        "constant",
        "variable.other.constant",
        "constant.other.symbol",
        "constant.language.symbol",
