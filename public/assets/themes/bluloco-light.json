      "settings": {
        "foreground": "#823FF1"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#275FE4"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "string.unquoted.alias.graphql"],
      "settings": {
        "foreground": "#DF631C"
      }
    },
    {
      "scope": "meta.selector",
      "settings": {
        "foreground": "#7A82DA"
      }
    },
    {
      "scope": "none",
      "settings": {
        "foreground": "#CE33C0"
      }
