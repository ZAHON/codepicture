      "settings": {
        "foreground": "#9CABCA"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag.sgml"],
      "settings": {
        "foreground": "#E6C384"
      }
    },
    {
      "scope": ["entity.name.tag.html"],
      "settings": {
        "foreground": "#957FB8"
      }
