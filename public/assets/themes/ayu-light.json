{
  "name": "ayu-light",
  "type": "light",
  "colors": {
    "--sandbox-editor-background": "#fcfcfc",
    "--sandbox-editor-foreground": "#5c6166",
    "--sandbox-editor-cursor-foreground": "#ffaa33",
    "--sandbox-editor-selection-background": "#035bd626",
    "--sandbox-editor-line-number-inactive-foreground": "#8a919966",
    "--sandbox-window-header-background": "#f8f9fa",
    "--sandbox-window-tab-active-foreground": "#5c6166",
    "--sandbox-window-tab-inactive-foreground": "#8a9199"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#787B8099",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#86B300"
      }
    },
    {
      "scope": ["string.regexp", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#4CBF99"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#A37ACC"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#A37ACC"
      }
    },
    {
      "scope": ["variable", "variable.parameter.function-call"],
      "settings": {
        "foreground": "#5C6166"
      }
    },
    {
      "scope": ["variable.member"],
      "settings": {
        "foreground": "#F07171"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#55B4D4",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["storage"],
      "settings": {
        "foreground": "#FA8D3E"
      }
    },
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#FA8D3E"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ED9366"
      }
    },
    {
      "scope": ["punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#5C6166B3"
      }
    },
    {
      "scope": ["punctuation.section"],
      "settings": {
        "foreground": "#5C6166"
      }
    },
    {
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#ED9366"
      }
    },
    {
      "scope": ["punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#FA8D3E"
      }
    },
    {
      "scope": ["punctuation.section.embedded"],
      "settings": {
        "foreground": "#FA8D3E"
      }
    },
    {
      "scope": ["meta.embedded"],
      "settings": {
        "foreground": "#5C6166"
      }
    },
    {
      "scope": ["source.java storage.type", "source.haskell storage.type", "source.c storage.type"],
      "settings": {
        "foreground": "#399EE6"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#55B4D4"
      }
    },
    {
      "scope": ["storage.type.function"],
      "settings": {
        "foreground": "#FA8D3E"
      }
    },
    {
      "scope": ["source.java storage.type.primitive"],
      "settings": {
        "foreground": "#55B4D4"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#F2AE49"
      }
    },
    {
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#A37ACC"
      }
    },
    {
      "scope": [
        "variable.function",
        "variable.annotation",
        "meta.function-call.generic",
        "support.function.go"
      ],
      "settings": {
        "foreground": "#F2AE49"
      }
    },
    {
      "scope": ["support.function", "support.macro"],
      "settings": {
        "foreground": "#F07171"
      }
    },
    {
      "scope": ["entity.name.import", "entity.name.package"],
      "settings": {
        "foreground": "#86B300"
      }
    },
    {
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#399EE6"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag.sgml"],
      "settings": {
        "foreground": "#55B4D4"
      }
    },
    {
      "scope": ["support.class.component"],
      "settings": {
        "foreground": "#399EE6"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#55B4D480"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#F2AE49"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#ED9366",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.type", "support.class", "source.go storage.type"],
      "settings": {
        "foreground": "#55B4D4"
      }
    },
    {
      "scope": [
        "meta.decorator variable.other",
        "meta.decorator punctuation.decorator",
        "storage.type.annotation"
      ],
      "settings": {
        "foreground": "#E6BA7E"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#E65050"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#C594C5"
      }
    },
    {
      "scope": ["source.ruby variable.other.readwrite"],
      "settings": {
        "foreground": "#F2AE49"
      }
    },
    {
      "scope": [
        "source.css entity.name.tag",
        "source.sass entity.name.tag",
        "source.scss entity.name.tag",
        "source.less entity.name.tag",
        "source.stylus entity.name.tag"
      ],
      "settings": {
        "foreground": "#399EE6"
      }
    },
    {
      "scope": [
        "source.css support.type",
        "source.sass support.type",
        "source.scss support.type",
        "source.less support.type",
        "source.stylus support.type"
      ],
      "settings": {
        "foreground": "#787B8099"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#55B4D4",
        "fontStyle": "normal"
      }
    },
    {
      "scope": ["constant.numeric.line-number.find-in-files - match"],
      "settings": {
        "foreground": "#787B8099"
      }
    },
    {
      "scope": ["constant.numeric.line-number.match"],
      "settings": {
        "foreground": "#FA8D3E"
      }
    },
    {
      "scope": ["entity.name.filename.find-in-files"],
      "settings": {
        "foreground": "#86B300"
      }
    },
    {
      "scope": ["message.error"],
      "settings": {
        "foreground": "#E65050"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "foreground": "#86B300",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#55B4D4"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#F07171",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#F07171",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic markup.bold", "markup.bold markup.italic"],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "scope": ["markup.raw"],
      "settings": {
        "background": "#5C616605"
      }
    },
    {
      "scope": ["markup.raw.inline"],
      "settings": {
        "background": "#5C61660F"
      }
    },
    {
      "scope": ["meta.separator"],
      "settings": {
        "foreground": "#787B8099",
        "background": "#5C61660F",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#4CBF99",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list punctuation.definition.list.begin"],
      "settings": {
        "foreground": "#F2AE49"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#6CBF43"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#478ACC"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF7383"
      }
    },
    {
      "scope": ["markup.strike"],
      "settings": {
        "foreground": "#E6BA7E"
      }
    },
    {
      "scope": ["markup.table"],
      "settings": {
        "foreground": "#55B4D4",
        "background": "#5C61660F"
      }
    },
    {
      "scope": ["text.html.markdown markup.inline.raw"],
      "settings": {
        "foreground": "#ED9366"
      }
    },
    {
      "scope": ["text.html.markdown meta.dummy.line-break"],
      "settings": {
        "foreground": "#787B8099",
        "background": "#787B8099"
      }
    },
    {
      "scope": ["punctuation.definition.markdown"],
      "settings": {
        "foreground": "#787B8099",
        "background": "#5C6166"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#316BCD"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#CD3131"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#800080"
      }
    }
  ]
}
